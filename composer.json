{
  "name": "natsimhan/simplex-noise",
  "description": "A speed-improved simplex noise algorithm for 2D in PHP.",
  "type": "library",
  "keywords": [
    "simplex",
    "noise",
    "generator"
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpunit/phpunit": "^10.0"
  },
  "license": "Unlicense",
  "autoload": {
    "psr-4": {
      "Natsimhan\\SimplexNoise\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Natsimhan\\SimplexNoise\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jonathan BURON aka Natsimhan",
      "email": "jonathan@ouebsson.fr"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  }
}
